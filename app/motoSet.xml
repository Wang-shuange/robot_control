<?xml version="1.0" encoding="UTF-8"?>                                      <!--这种尖括号结构中的内容是注释内容，不参与运算。-->
<motoSystem>                                                                <!--代码行设定了舵机的默认起始位置、最大极限工作角度、最小极限工作角度，舵机角度当量参数（大约是脉冲个数吧）。-->

	<motoSys id="31" name="点头" jdStart="90" jdMax="140" jdMin="40" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--大于140和小于40,硅胶皮脖子折弯明显，会干涉。-->

	<motoSys id="32" name="摇头" jdStart="90" jdMax="150" jdMin="30" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/> 
	</motoSys>                                                          <!--超过极限位置硅胶皮牵拉太严重 -->
	<motoSys id="33" name="摆头" jdStart="80" jdMax="150" jdMin="30" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!-- 大于150和小于30的时候，舵臂为无效摆动 -->
	<motoSys id="4" name="张口" jdStart="50" jdMax="80" jdMin="50" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--轴承中心回转半径14mm -->
	<motoSys id="5" name="眉左" jdStart="90" jdMax="120" jdMin="40" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--      -->
	<motoSys id="6" name="眉右" jdStart="90" jdMax="140" jdMin="60" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--      -->
	<motoSys id="7" name="眨左" jdStart="90" jdMax="150" jdMin="30" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--超过极限角度舵机臂会和桁架干涉，舵机动作最快为0.1s/60°，反向需要响应时间0.06s，极限短时眨眼循环周期0.32s-->
	<motoSys id="8" name="眨右" jdStart="90" jdMax="150" jdMin="30" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--超过极限角度舵机臂会和桁架干涉。-->
	<motoSys id="9" name="眼平" jdStart="90" jdMax="120" jdMin="60" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--调试后舵机极限角度吻合眼球球头枢轴的极限角度 -->
	<motoSys id="10" name="眼竖" jdStart="100" jdMax="130" jdMin="70" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--调试后舵机极限角度吻合眼球球头枢轴的极限角度（正常值是90°±30°，但存在装配误差。）-->
	<motoSys id="11" name="笑左" jdStart="45" jdMax="135" jdMin="45" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--      -->
	<motoSys id="12" name="笑右" jdStart="135" jdMax="135" jdMin="45" fScale="14.5" fOffSet="0">
		<motoImg jd="0" src="img1.jpg"/>
	</motoSys>                                                          <!--      -->
</motoSystem>
